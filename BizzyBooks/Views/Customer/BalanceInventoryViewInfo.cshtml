
@{
    ViewBag.Title = "BalanceInventoryViewInfo";
    Layout = null;
}

<div class="FlexPopup" style="display:block" ng-init="model = {}">
    <div class="flexheader lightblue">
        <label style="float:left;padding:15px ">

            <strong><span style="font-size:20px;  color:#585655">View Info</span></strong>
        </label>

        <label style="float:right;padding:15px">
            <span style="float:left; font-size:18px; margin-top:-2px">
                <label style="color:#000">State :</label>
                <span class="dropdown my" style="width:auto; margin-right:20px;">
                    <a href="#" class="dropdown-toggle stagesdrpdwn" data-toggle="dropdown" style="color:#ff9800">Closed<span class="caret" style="margin-top:0px;color:#000"></span></a>
                    <ul class="dropdown-menu dropmenu">
                        <li>Closed</li>
                        <li>Edit</li>
                        <li>New</li>
                        <li>Discard</li>
                    </ul>
                </span>
            </span>
            <strong style="font-size:24px; margin-top:-7px; float:right"><a href="" ng-click="goBack()"><i class="fa fa-times"></i></a></strong>


        </label>

    </div>
    <div class="flexcontainer">
        <div class="col-sm-4" style="min-height:560px; border-right:1px solid #808080; padding:0px 8px; background:rgba(158, 158, 158, 0.17)">


            <div style="height:850px" class="scrollbar scrollbarAngular">
                <div class="card  border-aero border-green">
                    <label class="heading">
                        Item Detail

                    </label>

                    <ul>

                        <li>
                            <span>Qty (KG)  </span>
                            <span class="pull-right">{{Item.NETWEIGHT}}(KG)</span>
                        </li>
                        <li>
                            <span>INVOICE NO </span>
                            <span class="pull-right">{{Item.no}}</span>
                        </li>

                        <li>
                            <span>Item Amount  </span>
                            <span class="pull-right">{{Item.AMOUNTINR}}</span>
                        </li>

                        <li>
                            <span>Cost/KG  </span>
                            <span class="pull-right">{{costPerMTinINR}}</span>
                        </li>


                    </ul>


                </div>
                <div class="card border-orange">
                    <label class="heading active">
                        Expense

                    </label>
                    <ul ng-repeat="data in expenseData">
                        <li>
                            <span>{{data.supliersName}}  </span>
                            <span class="pull-right"> {{data.amount/NetWeight |number : fractionSize}}</span>
                        </li>


                    </ul>
                </div>
                <div class="card border-orange">
                    <label class="heading active">
                        Tax & Duties

                    </label>
                    <ul>
                        <li>
                            <span>Custom Per KG </span>
                            <span class="pull-right"> {{totalCustom |number : fractionSize}}</span>
                        </li>


                    </ul>
                </div>
                <div class="card border-orange">
                    <label class="heading active">
                        UO Expense

                    </label>
                    <ul ng-repeat="data in taxData">
                        <li>
                            <span>{{data.accountName}}  </span>
                            <span class="pull-right"> {{data.credit/NetWeight |number : fractionSize}}</span>
                        </li>


                    </ul>
                </div>
                <div class="card border-blue">
                    <ul>
                        <li>
                            <span>
                                Total Expense
                                <span class="pull-right"><b>{{tatalExpense/NetWeight |number : fractionSize}}</b></span>
                            </span>
                        </li>
                    </ul>
                    <ul>
                        <li>
                            <span>
                                Total Cost/KG
                                <span class="pull-right"><b>{{totalCostPerMT |number : fractionSize}}</b></span>
                            </span>
                        </li>
                    </ul>
                </div>

            </div>
        </div>
